{
    "name": "aimfeld/zend-di-compiler",
    "description": "A Zend Framework 2 module that uses auto-generated factory code for dependency-injection.",
    "keywords": ["dependency injection", "dependency injection container", "DIC", "zend framework", "service manager", "zf2"],
    "homepage": "https://github.com/aimfeld/ZendDiCompiler",
    "type": "library",
    "license": "New BSD License",
    "authors": [
        {
            "name": "Adrian Imfeld",
            "email": "adrian.imfeld@cloud-solutions.net",
            "homepage": "http://www.cloud-solutions.net"
        }
    ],
    "require": {
        "php": ">=7.0",
        "zendframework/zend-config": "3.*",
        "zendframework/zend-di": "3.*",
        "zendframework/zend-servicemanager": "3.*"
    },
    "replace": {
        "aimfeld/di-wrapper": "*"
    },
    "autoload": {
        "psr-0": {
            "ZendDiCompiler": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
